{
  "name": "mikey",
  "version": "0.1.0",
  "description": "mikey software release planner",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run lint && npm run test && npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naxmefy/mikey.git"
  },
  "author": "MRW Neundorf <m.neundorf@live.de>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "6.14.0",
    "babel-loader": "6.2.7",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-runtime": "6.18.0",
    "better-npm-run": "0.0.13",
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "cors": "2.8.1",
    "css-loader": "0.26.0",
    "cssnano": "3.8.0",
    "debug": "2.3.2",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "fs-extra": "1.0.0",
    "html-webpack-plugin": "2.24.1",
    "imports-loader": "0.6.5",
    "ip": "1.1.4",
    "json-loader": "0.5.4",
    "node-sass": "3.13.0",
    "normalize.css": "5.0.0",
    "postcss-loader": "1.1.1",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.3",
    "yargs": "6.4.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-eslint": "7.1.1",
    "babel-plugin-istanbul": "3.0.0",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-enzyme": "0.6.0",
    "cheerio": "0.22.0",
